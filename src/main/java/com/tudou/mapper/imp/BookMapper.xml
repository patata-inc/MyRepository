<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tudou.mapper.imp.BookMapper2">

    <insert id="addBook" parameterType="com.tudou.vo.Book">
        insert into BOOK (id,name,author) values(#{id},#{name},#{author})
    </insert>
    <select id="getAllBooks" resultType="com.tudou.vo.Book">
        select * from book
    </select>
</mapper>
